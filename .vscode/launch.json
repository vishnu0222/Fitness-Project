{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS Application",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/dist/main.js",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "preLaunchTask": "npm: build",
        "sourceMaps": true
      }
    ]
  }